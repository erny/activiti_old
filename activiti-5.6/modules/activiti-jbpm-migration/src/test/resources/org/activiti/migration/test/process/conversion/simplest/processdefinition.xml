<?xml version="1.0" encoding="UTF-8"?>

<process-definition 
  xmlns="urn:jbpm.org:jpdl-3.2"
  name="simplest">
   <start-state name="start">
      <transition name="to_state" to="wait"/>
   </start-state>
   <state name="wait">
      <transition name="to_end" to="end" />
   </state>
   <end-state name="end"></end-state>
</process-definition>