<?xml version="1.0" encoding="UTF-8"?>
<activiti-cfg>
  
  <database type="@db@" schema-strategy="check-version">
    <jdbc url="@jdbc.url@"
          driver="@jdbc.driver@"
          username="@jdbc.username@"
          password="@jdbc.password@" />
  </database>
  
  <job-executor activate="on" />
  
  <mail server="localhost" port="5025" />
  
</activiti-cfg>
