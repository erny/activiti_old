<?xml version="1.0" encoding="UTF-8"?>

<definitions id="taskAssigneeExample" 
             xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
             xmlns:activiti="http://activiti.org/bpmn"
             targetNamespace="Upgrade">
  
  <process id="simpleTaskProcess">
  
    <startEvent id="start"/>
    
    <sequenceFlow id="flow1" sourceRef="start" targetRef="simpleTask" />

    <userTask id="simpleTask" name="simpleTask" />
    
    <sequenceFlow id="flow2" sourceRef="simpleTask" targetRef="end" />
    
    <endEvent id="end" />
    
  </process>

</definitions>